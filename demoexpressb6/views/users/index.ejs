<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>List Users</h1>
    <%= name %> 

    <ul>
        <% users.forEach(function(user){ %>
          <li>
            Name: <%= user.name %>  <br>
            Email: <%= user.email %> 
        <% }); %>
        </li>
      </ul>
</body>
</html>